	user = rlguarino

[push]
	default = upstream

[alias]
	assume = update-index --assume-unchanged
	unassume = update-index --no-assume-unchanged
	addumed = "!git ls-files -v | grep ^h | cut -c 3-"

[diff]
	algorithm = patience
