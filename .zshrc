# Source Prezto.
if [[ -s "${ZDOTDIR:-$HOME}/.zprezto/init.zsh" ]]; then
  source "${ZDOTDIR:-$HOME}/.zprezto/init.zsh"
fi

# Customize to your needs...
export PATH=$HOME/source/go/bin:$PATH

export GOPATH=$HOME/source/go

alias tmux="tmux -2"
alias emacs="emacs -nw"
